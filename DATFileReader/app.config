<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    
  </configSections>
  <connectionStrings>
    <!--<add name="TestDBConnection" connectionString="server=localhost;uid=devuser;password=admin@123;database=honda;pooling=true;charset=utf8mb4;Min Pool Size=5;Max Pool Size=50;SslMode=None;" />-->
    <!--<add name="TestDBConnection" connectionString="Server=120.77.40.245; Database=HONDA;uid=root;pwd=Jit@asdfgh;PORT=3306" />--> 
    <add name="TestDBConnection" connectionString="server=192.168.1.112;uid=root;password=root;port=3378;database=datdata;pooling=true;Min Pool Size=5;Max Pool Size=50;SslMode=None;" />
  </connectionStrings>
  <appSettings>
    <add key="dev" value="false" />
    <add key="DeviceNum" value="" />
    <add key="ScanInterval" value="190000" />
      <add key="ConnectionName" value="TestDBConnection"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v13.0" />
      </parameters>
    </defaultConnectionFactory>
  </entityFramework>
<system.data>
    



  <log4net>
    <appender name="Console" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%newline %date [%thread] - %message" />
      </layout>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Logs/INFO/" />
      <param name="DatePattern" value="yyyy-MM-dd'.log'" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <appender name="ErrorRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="Logs/ERROR/" />
      <param name="DatePattern" value="yyyy-MM-dd'.log'" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <logger name="ERROR">
      <level value="ERROR" />
      <appender-ref ref="Console" />
      <appender-ref ref="ErrorRollingLogFileAppender" />
    </logger>
    <logger name="INFO">
      <level value="INFO" />
      <appender-ref ref="Console" />
      <appender-ref ref="RollingLogFileAppender" />
    </logger>
  </log4net>

  <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.12.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories></system.data></configuration>